<div class="weapon-card item" data-item-id="{{_id}}" data-weapon-damage={{data.damage}}>
  <div class="weapon-info weapon-name">
    <p>
      <a class="item-edit" data-type="item">{{name}}</a>
    </p>
    <div class="weapon-stats">
    <p><strong>Damage:</strong> {{data.Damage}}</p>
    <p><strong>Durability:</strong> {{data.Durability}}</p>
    </div>
    <a class="item-delete">
      <i class="fas fa-trash">
      </i>
    </a>
  </div>
</div>